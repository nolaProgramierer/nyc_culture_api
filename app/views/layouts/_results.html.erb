<div id="eventBox">
    <dl>
      <!-- sort controller 'results' array of string hashes by key -->
      <% @results.sort_by do |listing| %>
          <dt><%= listing['event_name'] %>
          <dd class='venue'><%= listing['venue_name'] %>
          <dd class='address'><%= listing['street_address'] %>
          <dd class='when'><%= listing['date_time_description'] %>
          <dd class='description'><%= listing['web_description'].html_safe %>
      <% end %>
    </dl>
  </div><!--eventBox-->
